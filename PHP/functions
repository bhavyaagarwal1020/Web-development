<?php

function processMarks($marksArr){
    $sum = 0;
    foreach ($marksArr as $value) {
        $sum += $value;
    }
    return $sum;
}


$rohanDas = [34, 98, 45, 12, 98, 93];
$sumMarks = processMarks($rohanDas);
echo "Total marks scored by Rohan is $sumMarks <br>";

$harry = [99, 98, 93, 94, 17, 100];
$sumMarksHarry = processMarks($harry);

echo "Total marks scored by Harry is $sumMarksHarry";

?>
